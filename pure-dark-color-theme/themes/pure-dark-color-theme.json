  "tokenColors": [
    {
      "settings": {
        "foreground": "#f5f5ffff",
        //"background": "#263238ff"
      }
    },
    {
      "name": "Comment",
      "scope": [
        "comment",
        "punctuation.definition.comment"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#646E88"
      }
    },
    {
      "name": "Variables",
      "scope": [
        "variable",
        "string constant.other.placeholder"
      ],
      "settings": {
        "foreground": "#00f5f5"
      }
    },
    {
      "name": "Colors",
      "scope": [
        "constant.other.color",
        "punctuation"
      ],
      "settings": {
        "foreground": "#f5f5ff"
      }
    },
    {
      "name": "Invalid",
      "scope": [
        "invalid",
        "invalid.illegal"
      ],
      "settings": {
        "foreground": "#FD007F"
      }
    },
    {
      "name": "Storage, Keyword",
      "scope": [
        "keyword",
        "storage.type"
      ],
      "settings": {
      "fontStyle": "bold",
        "foreground": "#FB4B00"
      }
    },
    {
      "name": "Operator, Misc, Storage",
      "scope": [
        "constant.other.color",
        "meta.tag",
        "punctuation.definition.tag",
        "punctuation.separator.inheritance.php",
        "punctuation.definition.tag.html",
        "punctuation.definition.tag.begin.html",
        "punctuation.definition.tag.end.html",
        "punctuation.section.embedded",
        "keyword.other.template",
        "keyword.other.substitution",
        "storage.modifier",
        "constant.numeric"
      ],
      "settings": {
        "foreground": "#00f5f5"
      }
    },
        {
      "name": "Operator-1",
      "scope": [
        "keyword.control",
        "keyword.other.template",
        "keyword.other.substitution"
      ],
      "settings": {
        "foreground": "#FF8800"
      }
    },
            {
      "name": "Operator-2",
      "scope": [
        "keyword.other.unit",
        "keyword.other"
      ],
      "settings": {
      "fontStyle": "bold italic",
        "foreground": "#FF8800"
      }
    },
    {
      "name": "Tag",
      "scope": [
        "entity.name.tag",
        "meta.tag.sgml",
        "markup.deleted.git_gutter"
      ],
      "settings": {
        "foreground": "#f5f5ff"
      }
    },
    {
      "name": "Function, Special Method",
